<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foadia.champions">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeNotActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Ui.Main.MainCooking"></activity>
        <activity android:name=".Ui.Main.MainHome"></activity>
        <activity
            android:name=".Ui.Main.MainCountriesList"
            android:theme="@style/AppThemeNotActionBar"></activity>
        <activity android:name=".Ui.Main.MainSinging"/>
        <activity android:name=".Ui.Main.MainDrawing"/>
        <activity android:name=".Ui.Main.MainFilming"/>
        <activity
            android:name=".Ui.Main.MainDancing"
            android:theme="@style/AppThemeNotActionBar" />
        <activity
            android:name=".Ui.Main.MainRegister"
            android:theme="@style/AppThemeNotActionBar"></activity>
        <activity
            android:name=".Ui.Main.MainLogin"
            android:theme="@style/AppThemeNotActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Ui.Main.MainActivity" />
    </application>

</manifest>